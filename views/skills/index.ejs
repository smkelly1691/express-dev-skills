<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=, initial-scale=1.0">
    <title>Express Developer Skills</title>
</head>
<body>
    <h1>Developer Skills</h1>
    <ul>
        <% skills.forEach(function(s) { %>
            <li>
              <a href="/skills/<%= s.id %>"><%= s.skill %></a>
          
          <%= s.skill %>
            - 
          <%= s.done ? 'done' : 'not done' %>
        </li>
      <% }); %>
    </ul>
</body>
</html>